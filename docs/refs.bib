 journal = {Frontiers in neuroinformatics},
 doi = {10.3389/fninf.2013.00045},
 file = {Lowekamp, Chen et al. 2013 - The Design of SimpleITK:Attachments/Lowekamp, Chen et al. 2013 - The Design of SimpleITK.pdf:application/pdf}
}


