 issn = {1876-7931},
 journal = {Journal of Ultrasound},
 doi = {10.1007/s40477-020-00460-z},
 file = {Gervasio, Stelitano et al. 2020 - Carpal tunnel sonography:Attachments/Gervasio, Stelitano et al. 2020 - Carpal tunnel sonography.pdf:application/pdf}
